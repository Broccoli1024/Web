<%= form_for @post do |f| %>
    <%= f.label :title %>
      <%= f.text_field :title, :size => 140 %>
      <br>
    <%= f.label :image %>
      <%= f.file_field :image %>
      <br>
    <%= f.label :body %>
      <%= f.text_field :body, :size => 140 %> 
      <br>
    <%= f.submit "編集する" %>
  <% end %>